pipeline 
{
    agent any

    stages 
     {
         stage('Build') 
            {
            steps 
               {
                echo 'Building..'
               bat " mvn -f 'TestNGExperiment/pom.xml/pom.xml' clean install  "
               }
            }
        stage('Test') 
            {
            steps 
               {
                echo 'Testing..'
                bat "mvn test"
               }
            }
       stage('Compile') 
            {
            steps 
               {
                echo 'Compiling the project'
                bat "mvn compile"
               }
            }
        stage('Deploy') 
            {
            steps {
                   echo 'Deploying....'
                  }
            }
}
}
