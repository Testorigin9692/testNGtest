pipeline 
{
    agent any

    stages 
     {
         stage('Build') 
            {
            steps 
               {
                echo 'Building..'
               bat ' mvn -f TestNGExperiment//pom.xml clean install '
               }
            }
        stage('Test') 
            {
            steps 
               {
                echo 'Testing..'
                bat "mvn -f TestNGExperiment//pom.xml clean install"
               }
            }
       stage('Compile') 
            {
            steps 
               {
                echo 'Compiling the project'
                bat "mvn -f TestNGExperiment//pom.xml clean install"
               }
            }
        stage('Deploy') 
            {
            steps {
                   echo 'Deploying....'
                  }
            }
}
}
